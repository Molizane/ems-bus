{
    "comment": "Listagem dos formandos por ano/semestre",
    "owner": "SIGRA",
    "version": "1",
    "url": "/aluno/lista_formandos/tipo/",
    "async": "true",
    "url_callback": "/imprime_retorno_tela",
    "type": "GET",
    "params": [
        {
            "tipo": [
                "sintetico",
                "analitico"
            ]
        }
    ],
    "querystring": [
        {
            "name": "ano",
            "type": "year",
            "comment": "Ano de pesquisa"
        },
        {
            "name": "semestre",
            "type": "int",
            "comment": "Semestre de pesquisa"
        }
    ]
}
