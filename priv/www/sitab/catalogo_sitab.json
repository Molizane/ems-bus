[
	{
		"name": "/sitab/node_modules",
		"comment": "Bibliotecas node_modules do sitab",
		"owner": "sitab",
		"version": "1.0.0",
		"service" : "ems_static_file_service:execute",
		"url": "/sitab/node_modules/",
		"use_re" : true,
		"path" : "{{ sitab_api_management }}",
		"type": "GET",
		"cache_control" : "max-age=86400, public",
		"expires_minute" : 1440,
		"lang" : "erlang"
	},

	{
		"name": "/sitab/images",
		"comment": "image static files of sitb",
		"owner": "sitb",
		"version": "1.0.0",
		"service" : "ems_static_file_service:execute",
		"url": "/sitab/app/.*/img/",
		"use_re" : true,
		"path" : "{{ sitab_api_management }}",
		"type": "GET",
		"cache_control" : "max-age=3600, public",
		"expires_minute" : 60,
		"lang" : "erlang"
	},


	{
		"name": "redirect_url_sitab",
		"comment": "Redirect url /sitab to /sitab/index.html",
		"owner": "sitab",
		"version": "1.0.0",
		"service" : "ems_redirect_url_service:execute",
		"url": "/sitab",
		"redirect_url" : "/sitab/index.html",
		"path" : "{{ sitab_api_management }}",
		"type": "GET",
		"cache_control" : "no-cache",
		"expires_minute" : 0,
		"lang" : "erlang"
	},

	{
		"name": "/sitab",
		"comment": "Sistema sitab ErlangMS",
		"owner": "sitab",
		"version": "1.0.0",
		"service" : "ems_static_file_service:execute",
		"url": "/sitab/",
		"use_re" : true,
		"path" : "{{ sitab_api_management }}",
		"type": "GET",
		"cache_control" : "no-cache",
		"expires_minute" : 0,
		"lang" : "erlang"
	}
]
